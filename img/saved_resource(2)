function afp_show_flash (afp_src_url)
{
	var flashStr = "";
	flashStr+='<div style=" width:342px; height:270px;">';
	flashStr+='<div style=" float:left; width:19px;"><img src="http://imgdn3.soufunimg.com/2013/12/30/bj/gebz/39144c499d884a3db1c57d518a23ce8a.gif" usemap="#afp_fa_map_tag" style="cursor:pointer;height:269px;width:19px;border:0"></div>';
	flashStr+='<div style=" float:left; width:320px;">';
	flashStr+=' <div style="height:26px;"><img src="http://imgd1.soufunimg.com/2016/06/24/25k/73629e98c44e446ba5c3897b9bdcb56b.gif" usemap="#afp_fa_map_top" style="height:26px;width:320px;border:0"></div>';
	flashStr+='<div style="height:240px">';
	flashStr+='<div style="position:absolute; width:30px; height:20px; margin:220px 0 0 0px;background:url(http://imgd2.soufunimg.com/2015/12/25/25k/71ea846c543c4065b86e95b5b3244712.png); z-index:100;"></div>';
	if(afp_src_url.toLowerCase().indexOf("swf")>-1){
		flashStr+='  <div style="width:320px; height:240px;overflow:hidden;">';
		flashStr+=' <embed width="320" height="240" id="afp_fa_flash_object" plugspace="http://www.macromedia.com/shockwave/download/index.cgi?P1_Prod_Version=ShockwaveFlash" type="application/x-shockwave-flash" wmode="opaque" quality="autohigh" src="'+afp_src_url+'">';
		flashStr+=' </div>';
		flashStr+=' <div style="position:absolute; z-index:2; width:320px; height:240px; top:26px; left:19px;  visibility: visible;" id="afp_fa_blank_gif"><img width="320" height="240" border="0" style="cursor:pointer" onclick="Javascript:afp_ExCommand (\'link\');" src="http://imgdn.fang.com/blank.gif"></div>';
	}else{
		flashStr += "<img src='"+afp_src_url+"' alt='' onclick='Javascript:afp_ExCommand (\"link\");' border='0' style='cursor:pointer;width:320px;height:240px;'>"; 
	}
	flashStr+=' </div>';
	flashStr+='<div style="height:3px; font-size:3px;"><img src="http://imgdn0.soufunimg.com/2013/12/30/bj/gebz/21a83e6307c44340a372874006816a4c.gif" style="height:3px;width:320px;vertical-align:top;border:0" /></div>';
	flashStr+='</div>';
	flashStr+=' <div style=" float:left; width:3px;"> <img src="http://imgdn1.soufunimg.com/2013/12/30/bj/gebz/f3a655f918d44a078da4ae0e6ad93358.gif" style="height:269px;width:3px;border:0"></div></div>';
	flashStr += "<map name='afp_fa_map_tag'>";
	flashStr += "<area shape='poly' coords='16,157,1,173,0,249,15,265' onmouseover='Javascript:afp_ExCommand (\"open\");'>";//20,110,1,127,1,226,20,239
	flashStr += "</map>";
	flashStr += "<map name='afp_fa_map_top'>";
	flashStr += "<area shape='rect' coords='301,2,318,24'  style='cursor:pointer' onclick='Javascript:afp_ExCommand (\"close\");'>";
	flashStr += "</map>";
	document.getElementById("afp_fa_video").innerHTML = flashStr;
	setInterval("afp_faResize()",100);
}
function afp_faResize() 
{
	if(!document.getElementById("afp_fa_video"))
	{
		return;
	}
	var afp_divHeight = document.getElementById("afp_fa_video").offsetHeight;
	var afp_divWidth = document.getElementById("afp_fa_video").offsetWidth;
	var afp_docWidth = 0;
	var afp_docHeight = 0;
	var afp_scrollLeft = 0;
	var afp_scrollTop = 0;
	if(window.pageXOffset)
	{
		afp_scrollLeft=window.pageXOffset;
	}
	else if(document.documentElement&&document.documentElement.scrollLeft)
	{
		afp_scrollLeft=document.documentElement.scrollLeft;
	}
	else if(document.body)
	{
		afp_scrollLeft=document.body.scrollLeft; 
	}
	if(window.pageYOffset)
	{
		afp_scrollTop=window.pageYOffset;
	}
	else if(document.documentElement&&document.documentElement.scrollTop)
	{
		afp_scrollTop=document.documentElement.scrollTop;
	}
	else if(document.body)
	{
		afp_scrollTop=document.body.scrollTop;
	}
	if(window.innerWidth)
	{
		afp_docWidth=parseInt(window.innerWidth) - 17;
	}
	else if(document.documentElement&&document.documentElement.clientWidth)
	{ 
		afp_docWidth=document.documentElement.clientWidth;
	}
	else if(document.body)
	{
		afp_docWidth=document.body.clientWidth; 
	}
	if(window.innerHeight)
	{
		afp_docHeight=parseInt(window.innerHeight) - 17;
	}
	else if(document.documentElement&&document.documentElement.clientHeight)
	{ 
		afp_docHeight=document.documentElement.clientHeight; 
	}
	else if(document.body)
	{
		afp_docHeight=document.body.clientHeight;
	}
	
	document.getElementById("afp_fa_video").style.top = (parseInt(afp_docHeight) - parseInt(afp_divHeight) + parseInt(afp_scrollTop)) + "px";
	document.getElementById("afp_fa_video").style.left = (parseInt(afp_docWidth) - parseInt(afp_divWidth) + parseInt(afp_scrollLeft)) + "px";
}
	
function afp_faMoveLeft()
{
	var step = 50;
	afp_yoyi_opening = true;
	var afp_divLefts = parseInt(document.getElementById("afp_fa_video").style.left); 
	var afp_divWidths = parseInt(document.getElementById("afp_fa_video").style.width);
	if (342 - afp_divWidths < 50)
	{
		step = 342 - afp_divWidths;
		window.clearInterval(afp_objTimer);
		afp_yoyi_opening = false;
		afp_yoyi_open = true;
	}
	
	document.getElementById("afp_fa_video").style.left = (afp_divLefts - step) + "px"; 
	document.getElementById("afp_fa_video").style.width = (afp_divWidths + step) + "px";
}
function afp_faMoveRight()
{
	var step = 50;
	afp_yoyi_closeing = true;
	var afp_divLefts = parseInt(document.getElementById("afp_fa_video").style.left);
	var afp_divWidths = parseInt(document.getElementById("afp_fa_video").style.width);
	if (afp_divWidths <= 42)
	{
		step = afp_divWidths - 15;
		window.clearInterval(afp_objTimer);
		mark_three=2;
		afp_yoyi_closeing = false;
		afp_yoyi_close = true;
	}
	document.getElementById("afp_fa_video").style.width = (afp_divWidths - step) + "px";
	document.getElementById("afp_fa_video").style.left = (afp_divLefts + step) + "px";
	if (afp_divWidths <= 42)
	{
		afp_yoyi_open = false;
	}
}
function afp_faTracker(s)
{
	if (!document.getElementById("afp_fa_img"))
	{
		return;
	}
	if (afp_count_limit_cur <= afp_count_limit)
	{
		document.getElementById("afp_fa_img").src = s;
	}
}
function afp_ExCommand(command)
{
	if(command == "open" && afp_open_tab == 0 && afp_yoyi_open==false && afp_yoyi_close==true)
	{
		afp_open_tab = 1;
		afp_status = 0;
		afp_count_limit_cur ++;
		afp_faTracker(afp_time_url+"0");
		document.getElementById("afp_fa_video").style.width = "17px";
		document.getElementById("afp_fa_video").style.height = "285px";
		afp_faResize();
		afp_objTimer = window.setInterval("afp_faMoveLeft()",50);
		afp_objTimer100 = setTimeout("afp_ExCommand ('pos100')", afp_timeout * 1000);
	}
	else if(command == "pos100")
	{
		if (afp_status == 0)
		{
			afp_faTracker(afp_time_url+"100");
		}
		afp_objTimer = window.setInterval("afp_faMoveRight()",50) 
		afp_open_tab = 0;
	}
	else if(command == "close")
	{
		window.clearInterval(afp_objTimer100);
		afp_faTracker(afp_time_url+"-1");
		afp_objTimer = window.setInterval("afp_faMoveRight()",50) 
		afp_open_tab = 0;
		afp_status = -1;
		afp_yoyi_open = false;
		afp_yoyi_close = true;
	}
	else if(command == "share")
	{
		window.open("mailto:?subject=%B1%EA%CC%E2%A3%BA%CD%C6%BC%F6%BE%AB%C3%C0Flash&body=%C4%DA%C8%DD%A3%BA%C4%FA%B5%C4%BA%C3%D3%D1%CE%AA%C4%FA%CD%C6%BC%F6%C1%CB%D2%BB%B8%F6Flash%B6%CC%C6%AC%2C%C4%FA%BF%C9%D2%D4%B5%E3%BB%F7%C8%E7%CF%C2%B5%D8%D6%B7%D0%C0%C9%CD%3A"+window.location);
	}
	else if(command == "link")
	{
		window.open(afp_link_url);
	}
	else if(command == "download")
	{
		window.open(afp_src_url);
	}
	return 1;
}
function afp_setOpen_flash(){
	afp_window_height = jQuery(window).height();
	afp_window_objTime = window.setInterval('afp_open_flash()',50);
}
function afp_open_flash(){
	if(afp_window_scrollTop<afp_window_height){
		afp_window_scrollTop = jQuery(window).scrollTop();
	}else{
		window.clearInterval(afp_window_objTime);
		setTimeout("afp_ExCommand ('open')", 2000);
	}
}

if(afp_src_url!=""){
	afp_show_flash (afp_src_url);
}

afp_setOpen_flash();var detime = 6000;

function changediv_100035(){
	document.getElementById('QPhiddenLayer_100035').style.display = "block";
    document.getElementById('QPhiddenLayer_100035_clock').style.display="block";
    setTimeout("hidediv_100035()",detime)
}
function hidediv_100035(){
	 document.getElementById('QPhiddenLayer_100035').style.display="none";
	 document.getElementById('QPhiddenLayer_100035_clock').style.display="none";
}

function showfull_100035(){
	if(typeof jQuery=="function"){
        jQuery("#QPhiddenLayer_100035").slideDown(1500,function(){
            document.getElementById('QPhiddenLayer_100035_clock').style.display="block";
            window.setTimeout(function(){
                jQuery("#QPhiddenLayer_100035").slideUp(1500);
                document.getElementById('QPhiddenLayer_100035_clock').style.display="none";
            },detime);
        });
    }else{
        setTimeout("changediv_100035()",1000);
    }
}
function clock(){
	document.getElementById('QPhiddenLayer_100035').style.display="none";
    document.getElementById('QPhiddenLayer_100035_clock').style.display="none";
}
function QPhiddenLayer_100035_move(){
	document.getElementById('QPhiddenLayer_100035_clock').style.backgroundColor="#666";
}
function QPhiddenLayer_100035_out(){
	document.getElementById('QPhiddenLayer_100035_clock').style.backgroundColor="#ccc";
}
function addLoadEvent__yoyi(func) {
	var oldonload = window.onload;
	if (typeof window.onload != 'function') {
		window.onload = func;
	} else {  
		window.onload = function() {
			oldonload();
			func();
		}
	}
}	
addLoadEvent__yoyi(function(){
	if(typeof __full__yoyi=="undefined"){
		showfull_100035();
		__full__yoyi="true";
	}
});