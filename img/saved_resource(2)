function setCookies(name, value, expires, path, domain, secure) {
	document.cookie = name + "=" + escape(value)
			+ ((path) ? ";path=" + path : "")
			+ ((domain) ? ";domain=" + domain : "")
			+ ((secure) ? ";secure" : "");
}
var backurl = location.href;
var str = new Array();
str = backurl.split("?");
if (str.length > 1) {
	var source = new Array();
	source = str[1].split("=");
	if (source.length > 1 && typeof (source[0]) != "undefined"
			&& source[0] == "sf_source") {
		setCookies("sf_source", source[1].toLowerCase(), false, "/", "fang.com", "");
	} else if (source.length > 1 && typeof (source[0]) != "undefined"
			&& source[0] == "s") {
		setCookies("s", source[1].toLowerCase(), false, "/", "fang.com", "");
	} else {
		setCookies("sf_source", "", false, "/", "fang.com", "");
		setCookies("s", "", false, "/", "fang.com", "");
	}
} else {
	setCookies("sf_source", "", false, "/", "fang.com", "");
	setCookies("s", "", false, "/", "fang.com", "");
}

