function zujiShowOrHide(){
	var flag = $("#zujiFlag").val();
	if(flag==1){
		$("#zujiFlag").val("0");
		$("#hideZuji").css("display","none");
		$("#moverside").removeClass("r455");
		$("#hideTabdiv").removeClass("rside");
		$("#hideTabdiv").addClass("rsideV01");
		
	}
	if(flag==0){
		$("#zujiFlag").val("1");
		$("#hideZuji").css("display","block");
		$("#moverside").addClass("r455");
		$("#hideTabdiv").removeClass("rsideV01");
		$("#hideTabdiv").addClass("rside");
		var ppCity=$("#hideCity").val();
		$.ajax({
			type:'get',
	        dataType:'jsonp',
	        url:"http://www1.fang.com/kanfangAndOrderV2/footPrint.do",
	        data:"city="+ppCity,
	        complete:function(){
	        },
	        error:function(){
	        	//alert('error3');
	        },
	        success:function(res){
	        	
	        	var newHouseArr=res.result.newhouse;
	        	var esfhouseArr=res.result.esfhouse;
	        	var renthouseArr=res.result.renthouse;
	            var newHouseUlId=$("#newhouseFootId");
	            var rentHouseUlId=$("#renthouseId");
	            var esfHouseUlId=$("#esfHouseFootId");
	            newHouseUlId.find("li").remove();
	            rentHouseUlId.find("li").remove();
	            esfHouseUlId.find("li").remove();
	            if(typeof(newHouseArr)!="undefined"){
	            	for(var i=0;i<newHouseArr.length;i++)
	 	            {
	 	             var val=newHouseArr[i];
	 	             if(i%2==0){
	 	                var liElt = $("<li class='mar15'> <div class='imgbox' id='dsy_D11_"+(09+i)+"' ><a href="+val.url+" target='_blank'><img  alt="+val.projectname+" width='207' height='138' border='0' src="+val.picUrl+"></a><div class='texta'><p><a href="+val.url+" target='_blank'>"+val.projectname+"</a><label><a href="+val.url+" target='_blank'>"+val.distinct+"</a></label></p><p>"+ val.price+"</p></div></div></li>");
	 	                 newHouseUlId.append(liElt);
	 	              }else
	 	              {
	 	                var liElt = $("<li> <div class='imgbox' id='dsy_D11_"+(09+i)+"' ><a href="+val.url+" target='_blank'><img  alt="+val.projectname+" width='207' height='138' border='0' src="+val.picUrl+"></a><div class='texta'><p><a href="+val.url+" target='_blank'>"+val.projectname+"</a><label><a href="+val.url+" target='_blank'>"+val.distinct+"</a></label></p><p>"+ val.price+"</p></div></div></li>");
	                     newHouseUlId.append(liElt);
	 	              }
	 	           }
	            }
	           
	            if(typeof(renthouseArr)!="undefined") {
	            	for(var i=0;i<renthouseArr.length;i++)
	 	           {
	 	              var val=renthouseArr[i];
	 	            // alert(val);
	 	               if(i%2==0)
	 	               {
	 	                 var liElt = $("<li class='mar15'><div class='rimgbox' id='dsy_D11_"+(25+i)+"'> <a href="+val.url+" target='_blank'><img  alt="+val.quarters+" width='207' height='138' border='0' src="+val.picUrl+"></a><div class='texta'><p><label><a href="+val.url+" target='_blank'>"+val.distinct+"</a></label><a href="+val.url+" target='_blank'>"+ val.quarters+"</a></p><p><label>"+val.shi+"ÊÒ"+val.ting+"Ìü</label>"+val.price+"Ôª/ÔÂ</p></div></div>");
	 	                 rentHouseUlId.append(liElt);
	 	              }else
	 	              {
	 	                var liElt = $("<li> <div class='rimgbox' id='dsy_D11_"+(25+i)+"' ><a href="+val.url+" target='_blank'><img  alt="+val.quarters+" width='207' height='138' border='0' src="+val.picUrl+"></a><div class='texta'><p><label><a href="+val.url+" target='_blank'>"+val.distinct+"</a></label><a href="+val.url+" target='_blank'>"+ val.quarters+"</a></p><p><label>"+val.shi+"ÊÒ"+val.ting+"Ìü</label>"+val.price+"Ôª/ÔÂ</p></div></div>");
	                     rentHouseUlId.append(liElt);
	 	              }
	 	           }
	            }
	           
	            if(typeof(esfhouseArr)!="undefined"){
	            	for(var i=0;i<esfhouseArr.length;i++)
	 	           {
	 	              var val=esfhouseArr[i];
	 	               if(i%2==0)
	 	               {
	 	                 var liElt = $("<li class='mar15'><div class='rimgbox' id='dsy_D11_"+(17+i)+"'><div class='other'>Ó¶½ğ0.5%</div><a href="+val.url +" target='_blank'><img  alt="+val.projectname+" width='207' height='138' border='0' src="+val.picUrl+"></a><div class='texta'><p><label><a href="+val.url+" target='_blank'>"+val.area+"©O</a></label><a href="+val.url+" target='_blank'>"+val.projectname+"</a></p><p><label>"+val.shi+"ÊÒ"+val.ting +"Ìü</label>"+val.price+"ÍòÔª</p></div></div>");
	 	                 esfHouseUlId.append(liElt);
